<% content_for :third_level_layout do %>  

    <div class="container">

	<% if @events.any? %>  	
    
        <table class="events table">
        	<thead>
        		<th>Customer</th>
        		<th>Event Name</th>
        		<th>When</th>
        	</thead>
        	<tbody>
        		<%= render partial: "event", collection: @events %>
        	</tbody>
       	</table>
        <%= will_paginate @events %>
        <% end %>

    </div>

<% end %> 

<%= render :template => 'layouts/product' %>